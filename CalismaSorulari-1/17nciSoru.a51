#include "aduc841.h"

//17-Bank 0’da olundugunu varsayarak asagidaki komutlarin sonuçlari açisindan bir fark olup olmadigini 
//belirleyiniz. Ayni komutlari Keil ortaminda yazip çalistirarak program belleginde kaplanan alan ve komut 
//isleme süresi açisindan farki inceleyiniz.
//mov 00h,#01h
//mov r0,#01h


org 0000h
	
	mov 00h,#01h
	mov r0,#01h

end